{
	"name": "python-dark-theme",
	"type": "dark",
	"colors": {
		"editor.background": "#252525",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#000000",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6A9955"
			}
		},

		{
			"name": "Keyword",
			"scope": [
				"keyword.other.python"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#d6a356"
			}
		},


		{
			"name": "Keyword",
			"scope": [
				"meta.function-call.arguments.python"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#cdff94"
			}
		},

		{
			"name": "Keyword",
			"scope": [
